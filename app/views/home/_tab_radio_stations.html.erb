<!-- Radio stations -->
<div class="tab-content hidden" id='tab-radio-stations'>
    <ul class="list-none">
    <%- @radio_stations.each do |radio_station| %>
      <li class="my-5">
        <div><%= image_tag("radio_station_logos/radio_2.png", width: '50px', height: '50px') %></div>
        <div><%= radio_station.name %></div>
        <span><%= radio_station.status %></span>
      </li>
    <% end %>
  </ul>
</div>
